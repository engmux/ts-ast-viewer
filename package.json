{
  "name": "ts-ast-viewer",
  "version": "0.9.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsherret/ts-ast-viewer.git"
  },
  "keywords": [
    "typescript",
    "ast",
    "viewer"
  ],
  "author": "David Sherret",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dsherret/ts-ast-viewer/issues"
  },
  "homepage": "http://ts-ast-viewer.com",
  "scripts": {
    "start": "yarn run copyLibFiles && yarn run createGetCompilerApi && react-scripts-ts start",
    "build": "react-scripts-ts build",
    "lint": "tslint src/**/*.ts",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "predeploy": "npm run build",
    "deploy": "npm run build && gh-pages -d build",
    "copyLibFiles": "ts-node --project tsconfig.scripts.json scripts/copyLibFiles.ts ",
    "createGetCompilerApi": "ts-node --project tsconfig.scripts.json scripts/createGetCompilerApi.ts"
  },
  "dependencies": {
    "@types/circular-json": "^0.4.0",
    "@types/inline-style-prefixer": "^3.0.1",
    "@types/react-redux": "^5.0.11",
    "@types/react-split-pane": "^0.1.10",
    "@types/react-treeview": "^0.4.0",
    "circular-json": "^0.4.0",
    "react": "^16.0.0",
    "react-ace": "^5.4.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts-ts": "2.8.0",
    "react-split-pane": "^0.1.68",
    "react-treeview": "^0.4.7",
    "redux": "^3.7.2",
    "typescript": "^2.8.1",
    "typescript-2.6.2": "npm:typescript@2.6.2",
    "typescript-2.7.2": "npm:typescript@2.7.2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.47",
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.2",
    "ts-node": "^5.0.1",
    "ts-simple-ast": "^10.3.0",
    "tslint-react": "^3.5.1"
  }
}
